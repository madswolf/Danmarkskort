<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>


<HBox  xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="bfst19.osmdrawing.ControllerBarPanel" fx:id="BarPanel">
   <children>
      <Button fx:id="menuButton" mnemonicParsing="false" text="Menu">
         <HBox.margin>
            <Insets right="5.0" />
         </HBox.margin>
      </Button>
      <TextField prefWidth="300.0" promptText="Adresse" />
      <Button fx:id="ruteButton" mnemonicParsing="false" text="Rute" onAction="#openRute">
         <HBox.margin>
            <Insets left="5.0" />
         </HBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
